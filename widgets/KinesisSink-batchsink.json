{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "AWS Kinesis Sink",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Stream Name",
          "name": "name"
        },
        {
          "widget-type": "textbox",
          "label": "Access ID",
          "name": "accessID"
        },
        {
          "widget-type": "textbox",
          "label": "Access Key",
          "name": "accessKey"
        },
        {
          "widget-type": "textbox",
          "label": "Shard count",
          "name": "shardCount"
        },
        {
          "widget-type": "select",
          "label": "Distribute",
          "name": "distribute",
          "widget-attributes": {
            "values": [
              "true",
              "false"
            ],
            "default": "true"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "string",
          "bytes"
        ],
        "schema-default-type": "string"
      }
    }
  ]
}
